@model IEnumerable<YoureOnBootsTrap.Models.Contenido>

<h2>Resultado de buscar "@ViewBag.Titulo"</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Ver contenido", "Details", new { id=item.Id })
        </td>
    </tr>
}

</table>
