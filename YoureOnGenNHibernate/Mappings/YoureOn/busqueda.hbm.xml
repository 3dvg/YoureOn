<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="YoureOnGenNHibernate"
		                   namespace="YoureOnGenNHibernate.EN.YoureOn">
		    
		    
			
				<class name="BusquedaEN" table="Busqueda"> 
				  	
						<id name="Id" column="idBusqueda" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
			
			
			
		  			
						<bag name="Contenido" table="[busqueda_contenido]"  >
							<key>
								
									<column name="FK_idBusqueda_idBusqueda"/> 
							</key>
      						<many-to-many class="YoureOnGenNHibernate.EN.YoureOn.ContenidoEN">
									<column name="FK_titulo_idContenido"/> 
      						</many-to-many>
    					</bag>
					
					
		
		 </class>	
		  
		  <query name="BusquedaENReadAllHQL">from BusquedaEN</query>
		</hibernate-mapping>
	