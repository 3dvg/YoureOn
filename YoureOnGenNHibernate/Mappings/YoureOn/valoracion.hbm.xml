<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="YoureOnGenNHibernate"
		                   namespace="YoureOnGenNHibernate.EN.YoureOn">
		    
		    
			
				<class name="ValoracionEN" table="Valoracion"> 
						<id name="Id" column="idValoracion"/> 
			
					
					<property name="Fecha" type="date"> 
						<column name="fecha"  not-null="true"/>
					</property>
					<property name="Nota" type="float"> 
						<column name="nota"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<bag name="Usuario" table="[valoracion_usuario]"  >
							<key>
								
									<column name="FK_idValoracion_idValoracion"/> 
							</key>
      						<many-to-many class="YoureOnGenNHibernate.EN.YoureOn.UsuarioEN">
									<column name="FK_emailUsuario_idUsuario"/> 
      						</many-to-many>
    					</bag>
					
					
					
					
		
		 </class>	
		  
		  <query name="ValoracionENReadAllHQL">from ValoracionEN</query>
		</hibernate-mapping>
	